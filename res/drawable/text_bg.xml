<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true">
  <shape>
      <gradient android:startColor="#f99507" android:endColor="#f59000"
      android:angle="45"/>
      <stroke android:width="3dip" android:color="#f99507"/>
      <corners android:radius="5dp"/>
      <padding android:left="5dp" android:top="5dp" android:right="5dp"
               android:bottom="5dp"/>
  
  </shape>
  
  </item>
  <item android:state_focused="true">
  <shape>
      <gradient android:startColor="#f99507" android:endColor="#f59000"
      android:angle="45"/>
      <stroke android:width="3dip" android:color="#f99507"/>
      <corners android:radius="5dp"/>
      <padding android:left="5dp" android:top="5dp" android:right="5dp"
               android:bottom="5dp"/>
  
  </shape>
  
  </item>
  <item>
  <shape>
      <gradient android:startColor="#f49610" android:endColor="#bf832c"
      android:angle="45"/>
      <stroke android:width="3dip" android:color="#00000000"/>
      <corners android:radius="5dp"/>
      <padding android:left="5dp" android:top="5dp" android:right="5dp"
               android:bottom="5dp"/>
  
  </shape>
  
  </item>
</selector>